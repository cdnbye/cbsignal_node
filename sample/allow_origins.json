{
  "servers": [{
    "server": {
      "port": 8000
    },
    "websockets": {
      "path": "/*",
      "maxPayloadLength": 65536,
      "idleTimeout": 240,
      "compression": false,
      "maxConnections": 0
    }
  }],

  "websocketsAccess": {
    "allowOrigins": ["https://xxx.com", "https://yyy.com"],
    "denyEmptyOrigin": true
  }
}
